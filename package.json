{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "payload": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "generate:types": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "create-first-user": "node scripts/create-first-user.js",
    "seed-homepage": "node -r ts-node/register seed-homepage.ts",
  "postinstall": "node scripts/fix-payload-codeeditor.js && payload generate:types",
    "docker:build": "docker build -t payload-nextjs .",
    "docker:run": "docker run -p 3000:3000 payload-nextjs",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up",
    "lighthouse": "lhci autorun",
    "test": "echo \"Add your test command here\"",
    "test:e2e": "echo \"Add your e2e test command here\"",
    "clean": "rm -rf .next && rm -rf node_modules && npm install",
    "update-deps": "npm update && npm audit fix",
    "security-audit": "npm audit"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.59.0",
    "@payloadcms/next": "3.59.0",
    "@payloadcms/richtext-lexical": "3.59.0",
    "@payloadcms/richtext-slate": "3.59.0",
    "@payloadcms/ui": "3.59.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "payload": "3.59.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
